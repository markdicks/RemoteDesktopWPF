<Window x:Class="RemoteDesktopWPF.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="400" Width="300">
    <StackPanel Margin="20">
        <TextBox Name="UserNameBox" ToolTip="Username" Margin="0,5"/>
        <TextBox Name="EmailBox" ToolTip="Email" Margin="0,5"/>
        <PasswordBox Name="PasswordBox" ToolTip="Password" Margin="0,5"/>
        <TextBox Name="FirstNameBox" ToolTip="First Name" Margin="0,5"/>
        <TextBox Name="LastNameBox" ToolTip="Last Name" Margin="0,5"/>
        <TextBox Name="AddressBox" ToolTip="Address" Margin="0,5"/>
        <Button Content="Register" Click="Register_Click" Margin="0,10"/>
    </StackPanel>
</Window>
